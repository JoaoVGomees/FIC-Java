public class Main {
    public static void main(String[] args) {
      
      int i = 1;
      
      while(i <= 30){
        System.out.print(i);
      
        if(i % 5 == 0) {
          System.out.println(" xaxa");
        }  
        i++;
        System.out.println(" ");
      }
      
    
  }
}
