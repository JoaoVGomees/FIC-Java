public class Main {
    public static void main(String[] args) {
      
      for (int i = 8; i <= 42; i++)
        System.out.println((i % 2 == 1) ? i : "");
         
      // if(i % 2 == 1) {
      //   System.out.println(i);
      // }
    }
}


